{
    "model": {
        "name": "InfoGAN",
        "args": {
            "img_channels": 1,
            "img_size": 28,
            "latent_dim": 100,
            "categorical_code_dim": 10,
            "continuous_code_dim": 2,
            "lambda_cat": 1,
            "lambda_cont": 0.1,
            "lr": 0.0002,
            "b1": 0.5,
            "b2": 0.999,
            "weight_decay": 1e-5
        }
    },
    "dataset": {
        "name": "MNIST",
        "img_size": 28,
        "img_channels": 1,
        "batch_size": 32,
        "train_val_split": 0.8,
        "download": true
    }
}
